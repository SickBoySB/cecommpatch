		<piece filename="models//constructions//cropFurrowsCapEnd.upm" n="0" e="0" s="1" w="0"/>
		<piece filename="models//constructions//cropFurrowsSingle.upm" n="0" e="0" s="0" w="0"/>
	</dynamics>  

	<dynamics name="Tram Rail">
		<piece filename="models//vehicles//trackTramX.upm" n="1" e="1" s="1" w="1"/>
