<dynamicsxml>

	<dynamics name="Gabion (High)" mode="fence">
	<piece filename="models//constructions//gabion.upm" n="0" e="0" s="0" w="0"/>
	<representativemodel filename="models//constructions//gabion.upm"/>
	</dynamics>

	<dynamics name="Brick Wall" mode="wall2">
	<piece filename="models//constructions//fences//brickWall00Rail.upm" n="0" e="0" s="0" w="0"/>
	<piece filename="models//constructions//fences//brickWall00Post.upm" n="0" e="0" s="0" w="0"/>    
	<representativemodel filename="models//constructions//fences//brickWall00Post.upm"/>
	</dynamics>

	<dynamics name="White Picket Fence" mode="wall">
	<piece filename="models//constructions//fences//whitePicketFenceRail02.upm" n="0" e="0" s="0" w="1"/>
	<piece filename="models//constructions//fences//whitePicketFencePost01.upm" n="0" e="0" s="0" w="0"/>
	<representativemodel filename="models//constructions//fences//whitePicketFencePost01.upm"/>
	</dynamics>
	
	<dynamics name="Fence" mode="wall">
	<piece filename="models//constructions//fences//basicRailFenceRail02.upm" n="0" e="0" s="0" w="1"/>
	<piece filename="models//constructions//fences//basicRailFencePost01.upm" n="0" e="0" s="0" w="0"/>
	<representativemodel filename="models//constructions//fences//basicRailFencePost01.upm"/>
	</dynamics>
	
	<dynamics name="Barbed Wire Fence" mode="wall">
		<piece filename="models//constructions//fences//barbedwireFenceRail02.upm" n="0" e="0" s="0" w="1"/>
		<piece filename="models//constructions//fences//barbedwireFencePost01.upm" n="0" e="0" s="0" w="0"/>
		<representativemodel filename="models//constructions//fences//barbedwireFencePost01.upm"/>
	</dynamics>
	
	<dynamics name="Rustic Fence" mode="wall">
		<piece filename="models//constructions//fences//rusticFenceRail01.upm" n="0" e="0" s="0" w="1"/>
		<piece filename="models//constructions//fences//rusticFencePost01.upm" n="0" e="0" s="0" w="0"/>
		<representativemodel filename="models//constructions//fences//rusticFencePost01.upm"/>
	</dynamics>

